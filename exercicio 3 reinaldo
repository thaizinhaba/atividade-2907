//3. Escrever um algoritmo para determinar o consumo médio de um automóvel sendo fornecida a distância total

programa
{
	
	funcao inicio()
	{
	real km, litros, consumo
			escreva("Escreva os km percorridos: \n")
			leia(km)
			escreva("Escreva a quantidade de litros: ")
			leia(litros)
			consumo = km/litros
			escreva("O gasto médio km/l é: ", consumo) 
	}
}
