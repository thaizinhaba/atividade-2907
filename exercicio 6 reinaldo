//trocar a variavel a pelo valor da b 

programa
{
	
	funcao inicio()
	{
	real a,b,c
		escreva("O valor de A: ")
		leia(a)
		escreva(
"O valor de B: ")
		leia(b)
		c = a
		a = b
		b = c
		escreva("O valor de A: " , a)
		escreva("O valor de B: " , c)
	
	}
}
